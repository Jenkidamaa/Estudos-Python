#programa ordenação das panquecas de acordo com o o diametro

#Entrada


def ordem(panquecas):
    armazena = int
    #a = variavel booleana para determinar o fim do processo
    panquecas = list(input())
    indice = int

    for i in range(1, len(panquecas), 1):
        if panquecas[i - 1] < panquecas[i]:
            a = True

    while a == False:
        armazena = max(panquecas)
        print("Posicionando a panqueca ", armazena)
        panquecas.insert(0, armazena)
        del(panquecas[len(panquecas)])
        print("Primeira inversao: ", panquecas)

        print("Segunda inversao: ", panquecas)

    if a == True:
        print(panquecas)


lista = list(input())
lista.remove(" ")
ordem(lista)
